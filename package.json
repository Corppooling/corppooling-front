    "@headlessui/vue": "^1.7.9",
    "axios": "^1.3.3",
    "luxon": "^3.3.0",
    "pinia": "^2.0.28",
    "primeicons": "^6.0.1",
    "primevue": "^3.23.0",
    "status-code-enum": "^1.0.0",
    "swiper": "^9.0.5",
